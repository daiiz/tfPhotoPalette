{
  "manifest_version": 2,
  "name": "tfPhotoPalette",
  "short_name": "tfPhotoPalette",
  "description": "tfPhotoPalette",
  "version": "0.0.2",
  "minimum_chrome_version": "38",

  "icons": {
    "16" : "icon.png",
    "48" : "icon.png",
    "128": "icon.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": ["storage", "<all_urls>"]
}
